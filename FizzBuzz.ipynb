    "n1 = input(\"Enter a number: \")\n",
    "n = int(n1)\n",
    "i = 1\n",
    "for i in range (n+1):\n",
    "    if(i%3 == 0 and i%5 == 0):\n",
    "        print(str(i) + \"Fizz Buzz\")\n",
    "    elif(i%3 == 0):\n",
    "        print(str(i) + \"Fizz\")\n",
    "    elif(i%5 == 0):\n",
    "        print(str(i) + \"Buzz\")\n",
    "    else:\n",
    "        print(i)"
